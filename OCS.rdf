    <owl:NamedIndividual rdf:about="https://www.edithlaszny.eu/OCS/data/#status_Group_ID000001">
        <rdf:type rdf:resource="https://www.edithlaszny.eu/OCS/data/#Status_Group"/>
        <data:competesWith rdf:resource="https://www.edithlaszny.eu/OCS/data/#prestige_ID000001"/>
