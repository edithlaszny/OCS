    <owl:NamedIndividual rdf:about="https://www.edithlaszny.eu/OCS/data/#labeling_Theory_ID000001">
        <rdf:type rdf:resource="https://www.edithlaszny.eu/OCS/data/#Labeling_Theory"/>
        <data:contributesToCausalEvent rdf:resource="https://www.edithlaszny.eu/OCS/data/#causal_Event_ID000098"/>
        <data:stigmatises rdf:resource="https://www.edithlaszny.eu/OCS/data/#deviance_ID000003"/>
