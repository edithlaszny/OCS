        <rdf:type rdf:resource="https://www.edithlaszny.eu/OCS/data/#Degradation_Ceremony"/>
        <data:contributesToCausalEvent rdf:resource="https://www.edithlaszny.eu/OCS/data/#causal_Event_ID000091"/>
