    <!-- https://www.edithlaszny.eu/OCS/data/#traditional_Authority_ID000001 -->

    <owl:NamedIndividual rdf:about="https://www.edithlaszny.eu/OCS/data/#traditional_Authority_ID000001">
        <rdf:type rdf:resource="https://www.edithlaszny.eu/OCS/data/#Traditional_Authority"/>
        <data:contributesToCausalEvent rdf:resource="https://www.edithlaszny.eu/OCS/data/#causal_Event_ID000004"/>
