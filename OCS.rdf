    <!-- https://www.edithlaszny.eu/OCS/data/#environmental_Sociology_ID000001 -->

    <owl:NamedIndividual rdf:about="https://www.edithlaszny.eu/OCS/data/#environmental_Sociology_ID000001">
        <rdf:type rdf:resource="https://www.edithlaszny.eu/OCS/data/#Environmental_Sociology"/>
        <data:contributesToCausalEvent rdf:resource="https://www.edithlaszny.eu/OCS/data/#causal_Event_ID000064"/>
        <data:mobilises rdf:resource="https://www.edithlaszny.eu/OCS/data/#natural_Environment_ID000001"/>
